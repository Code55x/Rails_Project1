<h2>Edit your profile</h2>
<%= form_for %>

<div class="field">
  <%= f.label :name %>
  <%= f.text_field :name %>
</div>
<div class="field">
  <%= f.label :address %>
  <%= f.text_area :address %>
</div>
<div class="field">
  <%= f.label :website %>
  <%= f.text_field :website %>
</div>
<div class="field">
  <%= f.label :phoneNumber %>
  <%= f.telephone_field :phoneNumber %>
</div>
<div>
  <%= f.label :description %>
  <%= f.text_area :description %>
</div>